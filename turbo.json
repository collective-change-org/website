{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"cache": true,
			"env": [
				"PAYLOAD_SECRET",
				"DATABASE_URI"
			]
		},
		"check-types": {
			"dependsOn": [
				"^check-types"
			]
		},
		"dev": {
			"persistent": true,
			"cache": false
		},
		"preview": {
			"dependsOn": [
				"build"
			],
			"persistent": true,
			"cache": false
		}
	}
}