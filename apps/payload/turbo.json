{
	"extends": [
		"//"
	],
	"tasks": {
		"build": {
			"dependsOn": [
				"^build",
				"check-types"
			],
			"outputs": [
				".next/**",
				"!.next/cache/**"
			]
		},
		"check-types": {
			"dependsOn": [
				"^check-types"
			],
			"inputs": [
				"src/**/*.ts",
				"src/**/*.tsx",
				"tsconfig.json",
				"package.json"
			]
		},
		"lint": {
			"dependsOn": [
				"^lint"
			]
		}
	}
}
