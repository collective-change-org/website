{
	"plugins": ["prettier-plugin-astro", "prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": "*.astro",
			"options": {
				"parser": "astro"
			}
		}
	],
	"tailwindConfig": "./tailwind.config.mjs",
	"tabWidth": 4,
	"useTabs": true,
	"semi": false,
	"bracketSpacing": true,
	"jsxBracketSameLine": true
}